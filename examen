select * from bd_facts.tbl_asesores ;

select * from bd_facts.tbl_clientes ;

select * from bd_facts.tbl_facturas ;

select tbl_asesores.numeroID, tbl_asesores.nombres, tbl_clientes.idAsesor
from tbl_asesores 
inner join tbl_clientes on tbl_asesores.idAsesor = tbl_clientes.idAsesor; 

select tbl_clientes.idCliente, tbl_clientes.nombrecompleto, tbl_facturas.idFactura, tbl_facturas.fechaEmision, tbl_facturas.fechaVencimiento, tbl_facturas.fechapagada
from tbl_facturas
inner join tbl_clientes on tbl_clientes.idCliente = tbl_facturas.idCliente;




insert into bd_facts . tbl_asesores ( idAsesor 
)values( '200' ) ;

select * from bd_facts.tbl_asesores
where idAsesor = 200 ;

select cantClientes, idAsesor
from tbl_asesores

